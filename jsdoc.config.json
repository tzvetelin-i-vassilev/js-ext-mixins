{
	"recurseDepth": 20,
	"source": {
		"include": ["src"],
		"includePattern": ".+\\.(m)js(doc)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"opts": {
		"recurse": true,
		"destination": "./docs/"
	},
	"plugins": [
		"plugins/markdown"
	],
	"markdown": {
		"hardwrap": false
	},
	"templates": {
		"default": {
			"includeDate": false,
			"outputSourceFiles": false
		}
	}
}
