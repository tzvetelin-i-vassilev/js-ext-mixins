{
    "env": {
        "browser": true,
        "node": true,
        "es2022": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "espree",
    "parserOptions": {
        "sourceType": "module",
        "requireConfigFile": false
    },
    "rules": {
        "no-prototype-builtins": "off",
        "no-unused-vars": "off"
    }
}
